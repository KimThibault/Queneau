#!/usr/bin/env python

import random
import math

allThePoems = open("queneau.txt","r")
#poem = content of queneau.txt in array form dims 14 x 10
poem = allThePoems.readlines()

# randomly generated poem
randomPoem = []

# loop over lines
for iLine in range(14)
# generate random number to choose which poem line comes from
# append line to build poem
    randomPoem.append(poem(iLine math.ceil(10*random()))


print randomPoem
